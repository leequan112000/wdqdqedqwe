{
  "name": "octavian",
  "version": "1.0.0",
  "description": "Cromatic Backend",
  "main": "server.ts",
  "scripts": {
    "prisma:generate:main": "prisma generate --schema=./prisma-main/schema.prisma",
    "prisma:generate:cro": "prisma generate --schema=./prisma-cro/schema.prisma",
    "build": "yarn prisma:generate:main && yarn prisma:generate:cro && tsc --project ./ && yarn copy-graphql",
    "start:dev": "concurrently \"yarn nodemon src/server.ts\" \"yarn generate --watch\" \"yarn generate:admin --watch\"",
    "start:prod": "node dist/server.js",
    "generate": "graphql-codegen",
    "generate:admin": "graphql-codegen --config ./codegen-admin.ts",
    "copy-graphql": "cp -r ./src/graphql/types ./dist/graphql/types && cp -r ./src/graphql-admin/types ./dist/graphql-admin/types",
    "db:migrate:main": "prisma migrate dev --create-only --schema=./prisma-main/schema.prisma",
    "db:migrate:cro": "prisma migrate dev --create-only --schema=./prisma-cro/schema.prisma",
    "db:push:main": "prisma db push --schema=./prisma-main/schema.prisma",
    "db:push:cro": "prisma db push --schema=./prisma-cro/schema.prisma",
    "prepare": "husky install",
    "test": "vitest -c ./vitest.config.unit.ts",
    "stripe:listen:local": "stripe listen --forward-to localhost:9000/webhook/stripe --skip-verify --events customer.subscription.updated,customer.subscription.deleted,checkout.session.completed",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org cromatic --project octavian ./dist && sentry-cli sourcemaps upload --org cromatic --project octavian ./dist",
    "format": "prettier src/**/*.{ts,queues.ts,service.ts,resolvers.ts,test.ts} --write"
  },
  "engines": {
    "node": "^18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cro-matic/octavian.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cro-matic/octavian/issues"
  },
  "homepage": "https://github.com/cro-matic/octavian#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^3.1.0",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-resolvers": "^3.1.0",
