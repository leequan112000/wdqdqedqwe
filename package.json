{
  "name": "octavian",
  "version": "1.0.0",
  "description": "Cromatic Backend",
  "main": "server.ts",
  "scripts": {
    "build": "yarn prisma generate && tsc --project ./ && yarn copy-graphql",
    "start:dev": "nodemon src/server.ts",
    "start:prod": "node dist/server.js",
    "generate": "graphql-codegen",
    "copy-graphql": "cp -r ./src/graphql/types ./dist/graphql/types",
    "db:migrate": "yarn prisma migrate dev --create-only && yarn prisma generate",
    "db:push": "yarn prisma db push"
  },
  "engines": {
    "node": "18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cro-matic/octavian.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cro-matic/octavian/issues"
  },
  "homepage": "https://github.com/cro-matic/octavian#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^3.1.0",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-resolvers": "^3.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "^0.0.5",
    "nodemon": "^2.0.20",
    "prisma": "^4.10.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/schema": "^9.0.16",
    "@graphql-tools/utils": "^9.2.1",
    "@prisma/client": "^4.10.1",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-ts": "^1.0.2",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.18",
    "graphql-type-json": "^0.3.2",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "ncp": "^2.0.0",
    "pandadoc-node-client": "^5.5.0",
    "stripe": "^11.11.0"
  }
}
