{
  "name": "octavian",
  "version": "1.0.0",
  "description": "Cromatic Backend",
  "main": "server.ts",
  "scripts": {
    "build": "yarn prisma generate && tsc --project ./ && yarn copy-graphql",
    "start:dev": "concurrently \"yarn nodemon src/server.ts\" \"yarn generate --watch\"",
    "start:prod": "node dist/server.js",
    "generate": "graphql-codegen",
    "copy-graphql": "cp -r ./src/graphql/types ./dist/graphql/types && cp -r ./src/graphql-admin/types ./dist/graphql-admin/types",
    "db:migrate": "yarn prisma migrate dev --create-only && yarn prisma generate",
    "db:push": "yarn prisma db push",
    "prepare": "husky install",
    "test": "jest",
    "stripe:listen:local": "stripe listen --forward-to localhost:9000/webhook/stripe --skip-verify --events customer.subscription.updated,customer.subscription.deleted,checkout.session.completed"
  },
  "engines": {
    "node": "^18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cro-matic/octavian.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cro-matic/octavian/issues"
  },
  "homepage": "https://github.com/cro-matic/octavian#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^3.1.0",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-resolvers": "^3.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.0",
    "@types/uuid": "^9.0.1",
    "concurrently": "^7.6.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.10.1",
